    "@vitest/ui": "*",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "*",
    "husky": "*",
    "lint-staged": "*",
    "prettier": "*",
    "ts-node": "*",
    "typescript": "*",
    "vitest": "*"
  }
}
